Here come the articles
    <%= link_to "New", new_article_path%>
<div class="articles">
<% @articles.order(created_at: :desc).each do |article| %>
    <div class="article">
        <h4><%= link_to article.title, article_path(article) %></h4>
        <p><%= article.content %></p>
        <p><%= article.public %></p>
        <p><%= article.publish_date %></p>
        <p><%= article.user.first_name %></p>
    </div>
<% end %>
</div>
